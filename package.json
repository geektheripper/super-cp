{
  "name": "super-cp",
  "version": "0.0.1",
  "description": "copy files to any storage.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "yarn run build && yarn run global",
    "build": "yarn run clean:some && tsc -p .",
    "global": "npm i -g && super-cp",
    "clean:some": "rm -rf ./lib",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib",
    "refresh": "yarn run clean:all && yarn install"
  },
  "bin": {
    "super-cp": "./bin/super-cp"
  },
  "files": [
    "bin",
    "lib",
    "package.json",
    "yarn.lock",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.geektr.co:geektr/super-cp.git"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "ali-oss": "^6.17.1",
    "commander": "^9.2.0",
    "escape-string-regexp": "^5.0.0",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "minimatch": "^5.1.0",
    "object-hash": "^3.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.3",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.merge": "^4.6.7",
    "@types/mime": "^2.0.3",
    "@types/node": "^12.12.17",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
